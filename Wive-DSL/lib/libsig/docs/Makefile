install-dist_devhelpDATA: $(dist_devhelp_DATA)
	@$(NORMAL_INSTALL)
	test -z "$(devhelpdir)" || $(MKDIR_P) "$(DESTDIR)$(devhelpdir)"
	@list='$(dist_devhelp_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(dist_devhelpDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(devhelpdir)/$$f'"; \
	  $(dist_devhelpDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(devhelpdir)/$$f"; \
	done

